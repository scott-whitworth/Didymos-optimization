// Config file for Didymos Optimization Project
// For more in-depth information, refer to config_readme.md

// Randomization seed, set to NONE for time(0) as seed
time_seed=NONE

// Initial start point for the algorithm, initial_start_file_address only used if random_start is set to "false"
// IMPORTANT - if random_start is false, ensure that the variable parameter numbers (such as number of optimized variables) is set to 19 and so on as file makes the assumption it follows the old parameter size loadout
random_start=true
initial_start_file_address=../OptimizedVector.bin

// Output frequency variables
write_freq=1
disp_freq=100

// Algorithm Solution demands
best_count=2
pos_threshold=1.0e-10
coast_threshold=1

// Anneal determination properties
anneal_factor=0.5
anneal_initial=0.01
change_check=100

// Mutation Rates and scale for specific parameter types
mutation_rate=0.15
double_mutate_rate=0.2
triple_mutate_rate=0.05
gamma_mutate_scale=10.0
tau_mutate_scale=10.0
coast_mutate_scale=10.0
triptime_mutate_scale=0.5
zeta_mutate_scale=1.57
beta_mutate_scale=3.14
alpha_mutate_scale=3.14

// Thruster: 0 is none and 1 is NEXT ion thruster
thruster_type=1

// Mass of the spacecraft and fuel (kg), fuel_mass determines wet_mass
dry_mass=2700
fuel_mass=300

// Determines initial velocity magnitude
// Mission is 4.676e6
c3energy=4.676e6
// Official DART impact velocity
v_impact=4.399378072e-08

// Various values that impact runge kutta
rk_tol=1e-12
f_min=1e-20
max_numsteps=1e9

// Asteroid position and velocity at time of impact
// Values based on date: 30 Sep 2022 19:55:00 UTC

r_fin_ast=1.0352402142370513704605627935962
theta_fin_ast=1.5907019223523019557653412903164E-01
z_fin_ast=-5.5419274024321327209996468354802E-02

vr_fin_ast=-2.5351243043038431947801073368368E-08
vtheta_fin_ast=2.2730599434226558668330582440391E-07
vz_fin_ast=7.2985441781536943212764895715355E-09

// Earth position and velocity at time of impact

r_fin_earth=1.001408033845595291211338917492E+00
theta_fin_earth=1.2786133299119839534796483349055E-01
z_fin_earth=-1.1953653999999999579177505093508E-05

vr_fin_earth=-3.3052791656375833206445288103817E-09
